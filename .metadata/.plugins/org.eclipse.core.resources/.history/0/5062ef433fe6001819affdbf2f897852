package ScannerTutorial;

import org.omg.Messaging.SyncScopeHelper;

public class Test {



    public static void main(String[] args) 
            {
       int[] a={10,20,30,40};  
     for (int i=0;i<a.length;i++) 
            {
   System.out.print(" "+a[i]);
          } 
     System.out.println();
            
            for (int j=0;j<a.length;j++)
            {
            	System.out.print(" "+a[j]);
            	
            }
            } }